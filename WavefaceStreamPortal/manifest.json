{
    "name": "Stream Visited Links - a better history",
    "version": "__VERSION__",
    "default_locale": "en",
    "manifest_version": 2,
    "background": {
        "scripts": [
          "lib/jquery/jquery.js",
          "lib/jquery/purl.js",
          "lib/moment.js",
          "lib/wfSettings.js",
          "lib/wfAuth.js",
          "TabController.js",
          "HistoryExporter.js"
        ]
    },
    "permissions": [
        "tabs",
        "geolocation",
        "history",
        "http://*/*",
        "https://*/*"
    ],
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "19": "icon19.png"
    },
    "browser_action": {
        "default_title": "Stream Visited Links",
        "default_icon": "icon19.png",
        "default_popup": "ui/main.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["lib/jquery/jquery.js", "ContentManager.js"],
            "run_at": "document_start"
        }
    ]
}
