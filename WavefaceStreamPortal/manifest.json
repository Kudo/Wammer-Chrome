{
    "name": "Stream Portal",
    "version": "__VERSION__",
    "default_locale": "en",
    "manifest_version": 2,
    "background": {
	"scripts": ["waveface.js"]
    },
    "minimum_chrome_version": "13",
    "permissions": [
	"tabs",
	"http://*/*",
	"https://*/*"
    ],
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "StreamPhotoCollector_128.png",
        "48": "StreamPhotoCollector_48.png",
        "19": "StreamPhotoCollector_19.png"
    },
    "browser_action": {
        "default_title": "Stream Portal",
        "default_icon": "StreamPhotoCollector_19.png"
    }
}
