{
    "name": "Stream Visited Links - a better history (Dev)",
    "version": "__VERSION__",
    "default_locale": "en",
    "manifest_version": 2,
    "background": {
        "scripts": [
          "lib/jquery.js",
          "lib/moment.js",
          "lib/wfSettings.js",
          "lib/wfAuth.js",
          "TabController.js",
          "HistoryExporter.js"
        ]
    },
    "permissions": [
        "tabs",
        "geolocation",
        "history",
        "http://*/*",
        "https://*/*"
    ],
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "StreamPortal_128.png",
        "48": "StreamPortal_48.png",
        "19": "StreamPortal_19.png"
    },
    "browser_action": {
        "default_title": "Stream Visited Links (Dev)",
        "default_icon": "StreamPortal_19.png",
        "default_popup": "ui/profile.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["lib/jquery.js", "ContentManager.js"],
            "run_at": "document_start"
        }
    ],
    "update_url": "http://develop.waveface.com:4343/extensions/chrome/WavefaceStreamPortal/updates_dev.xml"
}
