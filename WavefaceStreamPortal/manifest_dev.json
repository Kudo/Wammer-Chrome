{
    "name": "Stream Portal (Dev)",
    "version": "__VERSION__",
    "default_locale": "en",
    "manifest_version": 2,
    "background": {
	"scripts": ["jquery.js", "TabController.js"]
    },
    "permissions": [
	"tabs",
	"geolocation",
	"http://*/*",
	"https://*/*"
    ],
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "StreamPhotoCollector_128.png",
        "48": "StreamPhotoCollector_48.png",
        "19": "StreamPhotoCollector_19.png"
    },
    "browser_action": {
        "default_title": "Stream Portal (Dev)",
        "default_icon": "StreamPhotoCollector_19.png"
    },
    "content_scripts": [
	{
	    "matches": ["http://*/*", "https://*/*"],
	    "js": ["jquery.js", "ContentManager.js"]
	}
    ],
    "update_url": "http://develop.waveface.com:4343/extensions/chrome/WavefaceStreamPhotoCollector/updates_dev.xml"
}
