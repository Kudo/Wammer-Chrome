{
    "name": "Stream Photo Collector",
    "version": "__VERSION__",
    "manifest_version": 2,
    "default_locale": "en",
    "background": {
	"scripts": ["waveface.js"]
    },
    "minimum_chrome_version": "13",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "StreamPhotoCollector_128.png",
        "48": "StreamPhotoCollector_48.png",
        "19": "StreamPhotoCollector_19.png"
    },
    "browser_action": {
        "default_title": "Stream Photo Collector",
        "default_icon": "StreamPhotoCollector_19.png"
    }
}
